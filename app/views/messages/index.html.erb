<%=render "layouts/shared/content/content_box", title: "Incoming/Outgoing Messages" do%>

    <div class="box-body table-responsive no-padding">
        <table class="table table-hover">
        <tr>
            <th></th>
            <th>Trans Set</th>
            <th>Description</th>
            <th>Trading Partner</th>
            <th>Received On</th>
            <th>Status</th>
        </tr>
    <%@messages.each do |message|%>
        <%=render "messages/message_table", message: message%>
    <%end%>
        </table>
    </div>

<%end%>